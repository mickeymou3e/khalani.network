{
  "name": "axi-syntax-highlighting",
  "displayName": "T-Axi Syntax Highlighting",
  "description": "Syntax highlighting for T-Axi",
  "version": "0.1.0",
  "publisher": "khalani",
  "repository":
  {
    "type": "git",
    "url": "git+https://github.com/tvl-labs/T-Axi.git"
  },
  "engines":
  {
    "vscode": "^1.80.0"
  },
  "categories": ["Programming Languages"],
  "contributes":
  {
    "languages":
    [{
      "id": "axi",
      "extensions": [".axi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars":
    [{
      "language": "axi",
      "scopeName": "source.axi",
      "path": "grammars/axi.tmLanguage.json"
    }],
    "themes":
    [
      {
        "label": "Axi Quiet Light Theme",
        "uiTheme": "vs",
        "path": "themes/axi-color-theme-quiet-light.json"
      }
    ]
  }
}
